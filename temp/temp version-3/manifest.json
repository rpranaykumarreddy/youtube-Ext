{
    "manifest_version": 3,
    "name": "YouTube Length calculator",
    "version": "0.1",
    "description": "This extension shows time taken to see the videos",
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["content.js"]
    }],
    "web_accessible_resources": [{
        "resources": ["style.css"],
        "matches": ["https://www.youtube.com/*"]
    }],
    "permissions": ["scripting", "tabs"]
}